($border-radius);\r\n        background-color: if($theme == \"A\", $background-color-baseA, $background-color-baseB);\r\n    }\r\n    .important {\r\n        @include common_styles($border-radius);\r\n        background-color: if($theme == \"A\", $background-color-importantA, $background-color-importantB);\r\n    }\r\n    .warning {\r\n        @include common_styles($border-radius);\r\n        background-color: if($theme == \"A\", $background-color-warningA, $background-color-warningB);\r\n    }\r\n    .font {\r\n        .principal::first-letter{\r\n            font-weight: bold;\r\n        }\r\n        .secondary {\r\n            font-family: if($theme == \"B\", $principal-font, $secondary-font);\r\n            font-weight: if($theme == \"B\", normal, bold);\r\n            position: absolute;\r\n        }\r\n\r\n        font-family: $principal-font;\r\n    }\r\n    .shadow{\r\n        @include common_styles($border-radius, $box-shadow);\r\n    }\r\n}\r\n\r\nbody {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    width: 80vw;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    min-height: 100vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    @include item_style();\r\n\r\n    main {\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n    }\r\n\r\n    header {\r\n        @include header_footer_styles();\r\n        padding-bottom: 1rem;\r\n    \r\n        .logo {\r\n            width: 15vw;\r\n            height: 10vh;\r\n            display: flex;\r\n            align-items: center;\r\n        }\r\n        \r\n        img{\r\n            width: 100%;\r\n            height: auto;\r\n        }\r\n    }\r\n\r\n    .item {\r\n        width: 100px;\r\n        height: 100px;\r\n    }\r\n\r\n    footer {\r\n        @include header_footer_styles();\r\n        margin-top: 1rem;\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n","_variables.scss":"$background-color-baseA: 